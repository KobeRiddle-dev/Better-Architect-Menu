<?xml version="1.0" encoding="utf-8"?>
<Patch>

<!-- ANIMAL BEDDING -->
	<Operation Class="PatchOperationAdd">
		<success>Always</success>
		<xpath>Defs/ThingDef[defName="Doghouse"]</xpath>
		<value>
			<designationCategory>Ferny_AnimalBedding</designationCategory>
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<success>Always</success>
		<xpath>Defs/ThingDef[defName="Dogbowl"]/designationCategory</xpath>
		<value>
			<designationCategory>Ferny_AnimalBedding</designationCategory>
		</value>
	</Operation>
	
</Patch>
