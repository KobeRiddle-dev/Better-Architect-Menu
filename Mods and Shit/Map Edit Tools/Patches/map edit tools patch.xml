<?xml version="1.0" encoding="utf-8"?>
<Patch>
    <Operation Class="PatchOperationReplace">
        <success>Always</success>
        <xpath>Defs/DesignationCategoryDef[defName="MapEdit"]/label</xpath>
        <value>
            <label>Map Edit</label>
        </value>
    </Operation>
    <Operation Class="PatchOperationAdd">
        <success>Always</success>
        <xpath>Defs/DesignationCategoryDef[defName="MapEdit"]</xpath>
        <value>
            <modExtensions>
                <li Class="BetterArchitect.NestedCategoryExtension">
                    <parentCategory>Ferny_Colony</parentCategory>
                </li>
                <li Class="BetterArchitect.SpecialCategoryExtension"></li>
            </modExtensions>
        </value>
    </Operation>
</Patch>