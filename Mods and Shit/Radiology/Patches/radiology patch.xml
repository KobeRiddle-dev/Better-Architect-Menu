<?xml version="1.0" encoding="utf-8"?>
<Patch>
	
	<!-- Consider putting this mod in Ferny_Enhancement, but it adds its own system and lots of buildings, so it gets its own category for now. -->
	
	<Operation Class="PatchOperationAddModExtension">
		<success>Always</success>
		<xpath>Defs/designationCategoryDef[defName="Radiology"]</xpath>
		<value>
			<li Class="BetterArchitect.NestedCategoryExtension">
				<parentCategory>Ferny_Biotech</parentCategory>
			</li>
		</value>
	</Operation>
	
</Patch>
