<?xml version="1.0" encoding="utf-8"?>
<Patch>

<!-- COOLING -->

	<Operation Class="PatchOperationReplace">
		<success>Always</success>
		<xpath>Defs/ThingDef[defName="WallLingCooler"]/designationCategory</xpath>
		<value>
			<designationCategory>Ferny_Cooling</designationCategory>
		</value>
	</Operation>

<!-- HEATING -->

	<Operation Class="PatchOperationReplace">
		<success>Always</success>
		<xpath>Defs/ThingDef[defName="WallHeater"]/designationCategory</xpath>
		<value>
			<designationCategory>Ferny_Heating</designationCategory>
		</value>
	</Operation>

</Patch>