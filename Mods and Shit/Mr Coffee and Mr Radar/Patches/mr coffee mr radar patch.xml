	</Operation>
	
<!-- OUTREACH -->

	<Operation Class="PatchOperationReplace">
		<success>Always</success>
		<xpath>Defs/ThingDef[defName="MRRADAR"]/designationCategory</xpath>
		<value>
			<designationCategory>Ferny_Outreach</designationCategory>
		</value>
	</Operation>
	
</Patch>
