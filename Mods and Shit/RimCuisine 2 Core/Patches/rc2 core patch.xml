<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="PatchOperationAddModExtension">
		<success>Always</success>
		<xpath>Defs/DesignationCategoryDef[defName="RC2_RimCuisine"]</xpath>
		<value>
			<li Class="BetterArchitect.NestedCategoryExtension">
				<parentCategory>Production</parentCategory>
			</li>
		</value>
	</Operation>

	<Operation Class="PatchOperationReplace">
		<success>Always</success>
		<xpath>Defs/ThingDef[designationCategory="RC2_RimCuisine"]/designationCategory</xpath>
		<value>
			<designationCategory>Ferny_Kitchen</designationCategory>
		</value>
	</Operation>
	
</Patch>
