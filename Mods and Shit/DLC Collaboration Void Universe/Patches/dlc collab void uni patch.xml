<?xml version="1.0" encoding="utf-8"?>
<Patch>
	
<!-- CONTROL -->
	
	<Operation Class="PatchOperationReplace">
		<success>Always</success>
		<xpath>Defs/ThingDef[defName="UV_BiomutationTerraformer"]/designationCategory</xpath>
		<value>
			<designationCategory>Ferny_Control</designationCategory>
		</value>
	</Operation>
	
<!-- SPACE PLATFORM -->

	<Operation Class="PatchOperationReplace">
		<success>Always</success>
		<xpath>Defs/ThingDef[defName="UV_VoidGravFieldExtender"]/designationCategory</xpath>
		<value>
			<designationCategory>Ferny_SpacePlatform</designationCategory>
		</value>
	</Operation>
	
</Patch>
