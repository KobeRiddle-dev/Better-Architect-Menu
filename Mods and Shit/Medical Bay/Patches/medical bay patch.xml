<?xml version="1.0" encoding="utf-8"?>
<Patch>
	
<!-- MEDICAL -->
	
	<Operation Class="PatchOperationReplace">
		<success>Always</success>
		<xpath>Defs/ThingDef[@Name="LTF_Medbay_Base"]/designationCategory</xpath>
		<value>
			<designationCategory>Ferny_Medical</designationCategory>
		</value>
	</Operation>
	
<!-- FILTH -->

	<Operation Class="PatchOperationReplace">
		<success>Always</success>
		<xpath>Defs/ThingDef[@Name="LTF_FilthCompressor_Base"]/designationCategory</xpath>
		<value>
			<designationCategory>Ferny_Filth</designationCategory>
		</value>
	</Operation>

	<Operation Class="PatchOperationAdd">
		<success>Always</success>
		<xpath>Defs/TerrainDef[defName="LTF_DrainFloor"]</xpath>
		<value>
			<designationCategory>Ferny_Filth</designationCategory>
		</value>
	</Operation>
	
</Patch>
