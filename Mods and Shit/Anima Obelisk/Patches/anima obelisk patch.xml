<?xml version="1.0" encoding="utf-8"?>
<Patch>
    <!-- Anima Obelisk by default is placed into "Misc" -->
    <Operation Class="PatchOperationReplace">
        <success>Always</success>
        <xpath>Defs/ThingDef[defName="AnimaObelisk"]/designationCategory</xpath>
        <value>
            <designationCategory>Ferny_Memorial</designationCategory>
        </value>
    </Operation>
</Patch>
