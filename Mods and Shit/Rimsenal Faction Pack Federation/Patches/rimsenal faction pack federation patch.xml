<?xml version="1.0" encoding="utf-8"?>
<Patch>

<!-- PRODUCTION -->
	
	<Operation Class="PatchOperationReplace">
		<success>Always</success>
		<xpath>Defs/ThingDef[defName="FedTable"]/designationCategory</xpath>
		<value>
			<designationCategory>Ferny_CombatEquipment</designationCategory>
		</value>
	</Operation>
	
<!-- WEAPONS -->

	<Operation Class="PatchOperationAdd">
		<success>Always</success>
		<xpath>Defs/ThingDef[defName="Turret_FedHarvester"]</xpath>
		<value>
			<designationCategory>Ferny_Weapons</designationCategory>
		</value>
	</Operation>

	<Operation Class="PatchOperationAdd">
		<success>Always</success>
		<xpath>Defs/ThingDef[defName="Turret_PB"]</xpath>
		<value>
			<designationCategory>Ferny_Weapons</designationCategory>
		</value>
	</Operation>
	
</Patch>