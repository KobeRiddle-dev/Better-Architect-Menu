<?xml version="1.0" encoding="utf-8" ?>
<Patch>


<!-- CHILDHOOD -->
                <Operation Class="PatchOperationAdd">
                    <success>Always</success>
                    <xpath>Defs/ThingDef[defName="RBM_MinotaurCrib"]</xpath>
                    <value>
                        <designationCategory>Ferny_Childhood</designationCategory>
                    </value>
                </Operation>


<!-- PRODUCTION -->
                <Operation Class="PatchOperationReplace">
                    <success>Always</success>
                    <xpath>Defs/ThingDef[defName="RBM_MilkingMachine"]/designationCategory</xpath>
                    <value>
                        <designationCategory>Ferny_Production</designationCategory>
                    </value>
                </Operation>

</Patch>