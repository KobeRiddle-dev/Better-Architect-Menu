<?xml version="1.0" encoding="utf-8"?>
<Patch>
	
<!-- DECORATIONS -->
	
	<Operation Class="PatchOperationReplace">
		<success>Always</success>
		<xpath>Defs/ThingDef[defName="BeachUmbrella"]/designationCategory</xpath>
		<value>
			<designationCategory>Ferny_Decorations</designationCategory>
		</value>
	</Operation>

<!-- SEATING -->
	
	<Operation Class="PatchOperationAdd">
		<success>Always</success>
		<xpath>Defs/ThingDef[defName="BeachChair"]</xpath>
		<value>
			<designationCategory>Ferny_Seating</designationCategory>
		</value>
	</Operation>
	
</Patch>
